<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ErrorWidget">
        <attr name="android:src" />
        <attr name="onClickRetry" />
        <attr name="message" format="string" />
        <attr name="button_text" format="string" />
        <attr name="footer_text" format="string" />
        <attr name="footer_visibility" format="enum">
            <enum name="visible" value="0x00000000" />
            <enum name="invisible" value="0x00000004" />
            <enum name="gone" value="0x00000008" />
        </attr>
    </declare-styleable>
</resources>